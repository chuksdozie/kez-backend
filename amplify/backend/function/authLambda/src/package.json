{
  "name": "authlambda",
  "version": "1.0.0",
  "description": "Lambda function generated by Amplify",
  "main": "index.js",
  "license": "Apache-2.0",
  "scripts": {
    "clean": "rm -rf dist",
    "buildb": "tsc -p ./tsconfig.json && cp -r dist/* .",
    "build": "tsc -p ./tsconfig.json && rm -rf build_package && mkdir build_package && cp -r dist/* build_package && cd build_package && zip -r ../latest-build.zip ./* && cd .. && rm -rf build_package",
    "amplify:authLambda": "cd amplify/backend/function/authLambda/src && yarn && tsc ./*.ts",
    "prepackage": "npm run build"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.883.0",
    "aws-serverless-express": "^3.3.5",
    "body-parser": "^1.17.1",
    "express": "^4.15.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.92",
    "@types/aws-serverless-express": "^3.3.10",
    "@types/node": "^24.3.1",
    "typescript": "^5.9.2"
  }
}
